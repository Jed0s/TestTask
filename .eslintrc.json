{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-var": "error",
        "indent": ["error", 4],
        "prefer-destructuring": ["error", {"object": false, "array": true}]
    }
}
